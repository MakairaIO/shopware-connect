<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="makaira_connect.subscriber.event.shopware"
                 class="MakairaConnect\Classes\Subscriber\ShopwareSubscriber">
        
            <tag name="shopware.event_subscriber" />
        </service>
        
        <service id="makaira_connect.subscriber.event.doctrine"
                 class="MakairaConnect\Classes\Subscriber\DoctrineSubscriber">
            
            <tag name="doctrine.event_subscriber" />
        </service>
    </services>
</container>
